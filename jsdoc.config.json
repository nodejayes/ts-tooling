{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "opts": {},
  "templates": {
    "name": "v3.7.0",
    "footerText": "  ",
    "useCollapsibles": true,
    "logo": {
      "url": "/ts-tooling.svg",
      "width": "150px",
      "height": "35px",
      "link": "https://github.com/nodejayes/ts-tooling"
    }
  }
}
