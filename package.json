{
  "name": "ts-tooling",
  "version": "1.0.6",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "mocha --require ts-node/register **/*.spec.ts",
    "type-generation": "dts-generator --project . --out index.d.ts",
    "doc": "typedoc --mode file --theme node_modules/typedoc-clarity-theme/bin --out documentation src/ts-tooling.ts",
    "build-test": "npm run build && npm run type-generation && npm run test",
    "release": "npm run build-test && npm publish"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "luxon": "^1.17.2",
    "async": "^3.1.0",
    "lz-string": "^1.4.4"
  },
  "devDependencies": {
    "typescript": "^3.5.3",
    "@types/node": "^12.7.2",
    "@types/lodash": "^4.14.136",
    "@types/luxon": "^1.15.2",
    "@types/async": "^3.0.1",
    "mocha": "^6.2.0",
    "chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "@types/chai": "^4.1.7",
    "ts-node": "^8.3.0",
    "nyc": "^14.1.1",
    "typedoc": "^0.15.0",
    "typedoc-clarity-theme": "^1.1.0",
    "dts-generator": "^3.0.0"
  }
}
