{
  "name": "ts-tooling",
  "version": "3.0.0-beta.3",
  "main": "src/ts-tooling.js",
  "types": "src/ts-tooling.d.ts",
  "scripts": {
    "test": "mocha --timeout 5000 ./src/**/*.spec.js",
    "cover": "nyc --reporter=lcov --report-dir coverage npm run test && npm run cover:publish",
    "cover:publish": "node ./scripts/coverage.js",
    "docs": "typedoc --out ./docs --mode file --excludePrivate --excludeNotExported --excludeNotExported --exclude node_modules ./src/ts-tooling.d.ts",
    "beforecommit": "npm run cover",
    "release": "npm run beforecommit && npm publish",
    "release:beta": "npm run beforecommit && npm publish --tag beta"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodejayes/ts-tooling"
  },
  "bugs": {
    "url": "https://github.com/nodejayes/ts-tooling/issues",
    "email": "markusgilg@outlook.de"
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "luxon": "^1.22.0",
    "immer": "^5.3.6",
    "lz-string": "^1.4.4",
    "rxjs": "~6.5.4",
    "ts-node": "^8.6.2",
    "uuid": "^3.4.0",
    "object-sizeof": "^1.5.2",
    "change-case": "^4.1.1"
  },
  "devDependencies": {
    "nyc": "^15.0.0",
    "mocha": "^7.0.1",
    "chai": "^4.2.0",
    "chai-uuid": "^1.0.6",
    "fs-extra": "^8.1.0",
    "typedoc": "^0.16.10",
    "readts": "^0.2.0",
    "lcov-parse": "^1.0.0",
    "lodash": "^4.17.15",
    "collatio": "^1.0.1"
  }
}
