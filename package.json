{
  "name": "ts-tooling",
  "version": "3.0.0",
  "main": "lib/ts-tooling.js",
  "types": "lib/ts-tooling.d.ts",
  "scripts": {
    "clear": "rimraf ./lib",
    "build": "npm run clear && webpack-cli",
    "test": "mocha --require ts-node/register --timeout 30000 **/*.spec.ts",
    "test:bundle": "mocha --timeout 30000 ./spec/bundle/**/*.spec.js",
    "test:sub": "mocha --timeout 30000 --watch ./spec/submodule/**/*.spec.js",
    "test:bundle:sub": "npm run test:bundle && npm run test:sub",
    "cover": "nyc --extension=.ts --extension=.js --reporter=lcov --report-dir coverage mocha --require ts-node/register --timeout 30000 **/*.spec.ts && npm run cover:publish",
    "cover:publish": "node ./scripts/coverage.js",
    "beforecommit": "npm run build && npm run cover && npm run docs",
    "release": "npm run beforecommit && npm publish",
    "docs": "typedoc --out ./docs --mode file --excludePrivate --excludeNotExported --excludeNotExported --exclude node_modules ./src/ts-tooling.ts"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodejayes/ts-tooling"
  },
  "bugs": {
    "url": "https://github.com/nodejayes/ts-tooling/issues",
    "email": "markusgilg@outlook.de"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "luxon": "^1.22.0",
    "immer": "^5.3.6",
    "lz-string": "^1.4.4",
    "rxjs": "~6.5.4",
    "ts-node": "^8.6.2",
    "uuid": "^3.4.0",
    "object-sizeof": "^1.5.2",
    "change-case": "^4.1.1"
  },
  "devDependencies": {
    "nyc": "^15.0.0",
    "mocha": "^7.0.1",
    "chai": "^4.2.0",
    "chai-uuid": "^1.0.6",
    "fs-extra": "^8.1.0",
    "typedoc": "^0.16.10",
    "lcov-parse": "^1.0.0",
    "lodash": "^4.17.15",
    "webpack-cli": "^3.3.11",
    "webpack": "^4.41.6"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "./src/ts-tooling.ts"
    },
    "dest": "dist"
  }
}
