{
  "name": "ts-tooling",
  "version": "2.4.3",
  "main": "lib/ts-tooling.js",
  "types": "lib/ts-tooling.d.ts",
  "scripts": {
    "build": "webpack-cli && npm run type-generation:main && npm run type-generation:nodeworker && npm run type-generation:webworker",
    "test": "mocha --require ts-node/register --timeout 30000 **/*.spec.ts",
    "cover": "nyc --extension=.ts --extension=.js --reporter=lcov --report-dir coverage mocha --require ts-node/register --timeout 30000 **/*.spec.ts",
    "coveralls": "npm run build && npm run cover && cat ./coverage/lcov.info | coveralls",
    "doc": "typedoc --mode modules --theme node_modules/typedoc-clarity-theme/bin --out docs src/ts-tooling.ts",
    "build-test": "npm run build && npm run test",
    "release": "npm run build-test && npm publish",
    "type-generation:main": "dts-generator --project . --out lib/ts-tooling.d.ts",
    "type-generation:nodeworker": "dts-generator --project . --out lib/node-worker.d.ts",
    "type-generation:webworker": "dts-generator --project . --out lib/web-worker.d.ts",
    "analyze": "webpack --profile --json > profiling.json && webpack-bundle-analyzer profiling.json"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodejayes/ts-tooling"
  },
  "bugs": {
    "url": "https://github.com/nodejayes/ts-tooling/issues",
    "email": "markusgilg@outlook.de"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "lodash": "^4.17.15",
    "luxon": "^1.21.3",
    "lz-string": "^1.4.4",
    "rxjs": "^6.5.3",
    "ts-node": "^8.5.4",
    "uuid": "^3.3.3",
    "object-sizeof": "^1.5.2"
  },
  "devDependencies": {
    "@types/async": "^3.0.1",
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.136",
    "@types/luxon": "^1.15.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "dts-generator": "^3.0.0",
    "fs-extra": "^8.1.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0",
    "typedoc-clarity-theme": "^1.1.0",
    "typescript": "~3.2.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
