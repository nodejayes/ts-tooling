{
  "name": "ts-tooling",
  "version": "2.1.0",
  "main": "lib/ts-tooling.bundle.js",
  "types": "lib/ts-tooling.d.ts",
  "scripts": {
    "build": "npm run clear && tsc && webpack-cli && npm run clean",
    "clean": "node ./scripts/clean_lib.js",
    "clear": "node ./scripts/clear_lib.js",
    "test": "mocha --require ts-node/register --timeout 30000 **/*.spec.ts",
    "cover": "nyc --extension=.ts --extension=.js --reporter=lcov --report-dir coverage mocha --require ts-node/register --timeout 30000 **/*.spec.ts",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls",
    "doc": "typedoc --mode file --theme node_modules/typedoc-clarity-theme/bin --out documentation src/ts-tooling.ts",
    "build-test": "npm run build && npm run test",
    "release": "npm run build-test && npm publish"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodejayes/ts-tooling"
  },
  "bugs": {
    "url": "https://github.com/nodejayes/ts-tooling/issues",
    "email": "markusgilg@outlook.de"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "async": "^3.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "lodash": "^4.17.15",
    "luxon": "^1.17.2",
    "lz-string": "^1.4.4",
    "rxjs": "^6.5.3",
    "ts-node": "^8.5.2",
    "uuid": "^3.3.3",
    "vinyl-source-stream": "^2.0.0"
  },
  "devDependencies": {
    "@types/async": "^3.0.1",
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.136",
    "@types/luxon": "^1.15.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0",
    "typedoc-clarity-theme": "^1.1.0",
    "typescript": "^3.5.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "fs-extra": "^8.1.0"
  }
}
